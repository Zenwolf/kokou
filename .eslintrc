{
    "globals": {
        "core": false,
        "kokou": false
    },

    "env": {
        "browser": true,
        "node": false,
        "amd": false
    },

    "rules": {
        "no-console": 1,
        "no-new-array": 1,
        "no-return-assign": 1,
        "no-spaced-func": 0,
        "no-ternary": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": 1,
        "one-var": 0,
        "quotes": [2, "single"],

        "consistent-this": [0, "that"],
        "max-params": [1, 5],
        "max-depth": [1, 3],
        "max-len": [1, 80, 4],
        "space-unary-word-ops": 2,
        "strict": 0

    }
}

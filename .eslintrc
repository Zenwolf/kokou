{
    "globals": {
        "core": false
    },

    "env": {
        "browser": true,
        "node": false,
        "amd": false
    },

    "rules": {
        "no-console": 1,
        "no-comma-dangle": 2,
        "no-debugger": 2,
        "no-empty": 2,
        "no-obj-calls": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "no-dupe-keys": 2,
        "no-empty-class": 2,
        "no-func-assign": 2,
        "no-control-regex": 2,

        "no-caller": 2,
        "curly": 2,
        "eqeqeq": 2,
        "dot-notation": 2,
        "no-catch-shadow": 2,
        "no-eval": 2,
        "no-with": 2,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-floating-decimal": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-native-reassign": 2,
        "no-delete-var": 2,
        "no-return-assign": 1,
        "no-label-var": 2,
        "wrap-iife": 2,
        "no-self-compare": 2,
        "no-eq-null": 2,
        "no-multi-str": 2,
        "no-loop-func": 2,
        "no-empty-label": 2,
        "unnecessary-strict": 2,
        "no-unused-expressions": 2,
        "no-unused-vars": 2,
        "no-script-url": 2,
        "no-proto": 2,
        "no-iterator": 2,
        "no-else-return": 0,
        "no-shadow": 2,
        "no-alert": 2,
        "no-use-before-define": 2,
        "no-redeclare": 2,
        "no-global-strict": 2,
        "strict": 0,
        "no-div-regex": 2,
        "no-fallthrough": 2,
        "no-ex-assign": 2,
        "no-sync": 2,
        "no-new-array": 1,

        "brace-style": 0,
        "camelcase": 2,
        "new-cap": 2,
        "quotes": [2, "single"],
        "quote-props": 0,
        "semi": 2,
        "no-ternary": 0,
        "consistent-this": [0, "that"],
        "one-var": 0,
        "no-mixed-requires": 0,
        "no-wrap-func": 2,
        "complexity": [2, 4],
        "new-parens": 2,
        "no-spaced-func": 0,
        "wrap-regex": 2,
        "radix": 0,
        "regex-spaces": 2,

        "no-plusplus": 0,
        "no-bitwise": 0,
        "guard-for-in": 0,
        "max-statements": [0, 100],
        "max-params": [2, 5],
        "max-depth": [2, 3],
        "max-len": [0, 500]
    }
}
